<Window x:Class="LIBRARY.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LIBRARY"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="350
">
    <Grid>
        <Label Content="Username" HorizontalAlignment="Left" Margin="79,84,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="userInput" HorizontalAlignment="Left" Height="23" Margin="147,86,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TabIndex="0"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="79,124,0,0" VerticalAlignment="Top"/>
        <Button Content="Login" HorizontalAlignment="Left" Margin="130,175,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" TabIndex="2"/>
        <PasswordBox x:Name="passInput" HorizontalAlignment="Left" Margin="147,124,0,0" VerticalAlignment="Top" Width="120" Height="23" TabIndex="1"/>

    </Grid>
</Window>
